@startuml

package "Colony" 
{
    package "Configs" 
    {
        class ColonyConfig 
        {
        }
    }
}
    package "Models" {
    class Colony 
    {
        +ColonyConfig config
        +ColonyDirector director
    }
    
    class ColonyDirector
    {
        +ColonyPopulation population
        +ColonyBuildings buildings
        +ColonyOrdersPool pool
    }
    
    class ColonyPopulation 
    {
        +List<Citizen> citizens
    }
    
    class ColonyBuildings 
    {
        +List<ProductionBuilding> productionBuildings
        +List<ServiceBuilding> serviceBuildings
        +List<StorageBuilding> storageBuildings
        +List<DecorBuilding> decorBuildings
    }

    Colony *-- ColonyDirector
    ColonyDirector *-- ColonyBuildings
    ColonyDirector *-- ColonyPopulation
    }
}

@enduml