@startuml

package "Resources" 
{
    package "Models"
    {
        class Resource
        {
            +ResourceConfig config
            +int amount
        }
    }

    package "Configs"
    {
        class ResourceConfigCollection
        {
            +Dictionary<string, ResourceConfig> configs
        }
            
        class ResourceConfig 
        {
            +string type
            +string name
        }
        
        ResourceConfigCollection o--> ResourceConfig 
    }
}

@enduml